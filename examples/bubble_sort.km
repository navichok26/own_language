
цел arr[5];
arr[0] = 5;
arr[1] = 3;
arr[2] = 8;
arr[3] = 4;
arr[4] = 1;

цел n = 5;

цел i = 0;
цел j = 0;
цел temp = 0;

нц_пока (i < (n - 1)) {
    j = 0;
    нц_пока (j < (n - i - 1)) {
        если (arr[j] > arr[j + 1]) {
            temp = arr[j];
            arr[j] = arr[j + 1];
            arr[j + 1] = temp;
        };
        j = j + 1;
    };
    i = i + 1;
};

вывод arr[0]; конецстр;
вывод arr[1]; конецстр;
вывод arr[2]; конецстр;
вывод arr[3]; конецстр;
вывод arr[4]; конецстр;