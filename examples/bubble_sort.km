
int arr[5];
arr[0] = 5;
arr[1] = 3;
arr[2] = 8;
arr[3] = 4;
arr[4] = 1;

int n = 5;

int i = 0;
int j = 0;
int temp = 0;

while (i < (n - 1)) {
    j = 0;
    while (j < (n - i - 1)) {
        if (arr[j] > arr[j + 1]) {
            temp = arr[j];
            arr[j] = arr[j + 1];
            arr[j + 1] = temp;
        };
        j = j + 1;
    };
    i = i + 1;
};

